model Instrument {
  ...id

  ///@Relation()
  study Study @relation(fields: [studyId], references: [id])
  ///@RelationId()
  studyId String

  name String
  title Json @default("{}")
  description Json @default("{}")

  content Json
  evaluations Json @default("{}")
  state Json @default("{}")

  schedule Json @default("{}")

  @@map("instruments")
}